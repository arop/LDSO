<%= render 'common/header' %>

</head>
<body>
<%= render 'common/navbar' %>

<div class="container">
<p id="notice"><%= notice %></p>

<h1> Os meus planos de reabilitação </h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reabilitacaos.each do |reabilitacao| %>
      <tr>
        <td><%= link_to 'Show', reabilitacao %></td>
        <td><%= link_to 'Edit', edit_reabilitacao_path(reabilitacao) %></td>
        <td><%= link_to 'Destroy', reabilitacao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reabilitacao', new_reabilitacao_path %>
<%= render 'common/footer' %>